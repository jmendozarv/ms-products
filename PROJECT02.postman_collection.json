				{
					"name": "Generate Token",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Obtiene la respuesta como un objeto JSON\r",
									"const response = pm.response.json();\r",
									"\r",
									"// Extrae el valor del campo \"token\"\r",
									"const token = response.token;\r",
									"\r",
									"// Almacena el token como una variable global\r",
									"pm.globals.set(\"myToken\", token);\r",
									"\r",
									"// Verifica si el token se ha almacenado correctamente\r",
									"pm.test(\"Token has been set globally\", function() {\r",
									"    pm.expect(pm.globals.get(\"myToken\")).to.eql(token);\r",
									"});\r",
									"\r",
									""
								],
